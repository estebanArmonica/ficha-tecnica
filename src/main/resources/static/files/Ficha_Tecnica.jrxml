	<parameter name="PACIENTE_ID" class="java.lang.Long">
        <parameterDescription><![CDATA[ID del paciente]]></parameterDescription>
    </parameter>
	
	<queryString language="SQL">
		<![CDATA[SELECT p.rut_paciente, p.nom_paciente, p.fech_nacimiento, 
					       CASE 
					         WHEN EXTRACT(MONTH FROM SYSDATE) > EXTRACT(MONTH FROM p.fech_nacimiento) 
					              OR (EXTRACT(MONTH FROM SYSDATE) = EXTRACT(MONTH FROM p.fech_nacimiento) 
					                  AND EXTRACT(DAY FROM SYSDATE) >= EXTRACT(DAY FROM p.fech_nacimiento))
					         THEN EXTRACT(YEAR FROM SYSDATE) - EXTRACT(YEAR FROM p.fech_nacimiento)
					         ELSE EXTRACT(YEAR FROM SYSDATE) - EXTRACT(YEAR FROM p.fech_nacimiento) - 1
					       END AS EDAD_PACIENTE, 
					        p.nro_paciente,
					       g.nom_genero, t.nom_tipo_sangre
					FROM pacientes p INNER JOIN genero g
					ON(g.id_genero = p.genero_id) INNER JOIN tipo_sangre t
					ON(t.id_tipo_sangre = p.tipo_sangre_id)
					WHERE p.id_paciente = $P{PACIENTE_ID}]]>
	</queryString>
